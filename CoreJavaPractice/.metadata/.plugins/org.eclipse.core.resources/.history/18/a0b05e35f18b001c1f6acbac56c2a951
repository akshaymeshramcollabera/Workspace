package programs;

class Test1{
    int a, b;

    Test1(int i, int j) {
        a = i;
        b = j;
    }

    void meth(Test1 of) {
        of.a = a * 2;
        of.b = b / 2;
    }
}
public class CallByRef {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
